header.header
  nav.nav.nav--tours
    a.nav__el(href="/") All tours
    form.nav__search
      button.nav__search-btn
        svg
          use(href="/img/icons.svg#icon-search")
      input.nav__search-input(type="text", placeholder="Search tours")
  .header__logo
    img(src="/img/logo-white.png", alt="Natours logo")
  nav.nav.nav--user
  
  if user
    a.nav__el(href="/login") Logout
    a.nav__el.nav__el--cta(href="/me") Daud
  else
    a.nav__el(href="/login") Login
    a.nav__el.nav__el--cta(href="/signup") Signup
